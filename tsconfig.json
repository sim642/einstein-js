{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.iterable",
      "es2015.symbol.wellknown",
      "es2015.promise",
      "es2015.core"
    ],
    "jsx": "react",
    "jsxFactory": "h",
    "sourceMap": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "allowJs": true,
    "importHelpers": true
  },
  "exclude": [
    "node_modules"
  ]
}